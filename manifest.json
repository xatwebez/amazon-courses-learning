{
  "manifest_version": 3,
  "name": "Element Queue",
  "version": "1.0",
  "description": "A Chrome extension to store and open elements from a webpage.",
  "permissions": ["activeTab", "storage","scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
